{{#if listExists}}
<table class="table mt-5">
    <tr>
        <th>Titulo</th>
        <th>Precio</th>
        <th>Foto</th>
    </tr>

    {{#each productos}}
        <tr class="align-middle">
            <td>{{this.title}}</td>
            <td>{{this.price}}</td>
            <td><img style="width:50px;height:50px;" src="{{this.thumbnail}}"></td>
        </tr>    
    {{/each}}  

{{else}}
  <div class="card-body mt-5 bg-info bg-gradient d-flex align-items-center justify-content-center">
    No se ha ingresado ningun producto
  </div>
{{/if}}
</table>
